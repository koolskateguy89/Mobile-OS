<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<fx:root contentDisplay="RIGHT" maxHeight="1.7976931348623157E308" stylesheets="@Subject.css" type="TitledPane" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <content>
      <AnchorPane maxHeight="1.7976931348623157E308" minHeight="0.0" minWidth="0.0" prefWidth="429.0">
         <children>
            <VBox fx:id="countWrapper" maxHeight="1.7976931348623157E308" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
         </children></AnchorPane>
   </content>
   <graphic>
      <HBox spacing="3.0">
         <children>
            <Button contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false" onAction="#edit">
               <graphic>
                  <ImageView fitHeight="12.0" fitWidth="22.0" pickOnBounds="true" preserveRatio="true">
                     <Image url="https://image.flaticon.com/icons/png/512/1250/1250615.png" />
                  </ImageView>
               </graphic>
            </Button>
            <Button maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#newCount" prefHeight="24.0" prefWidth="28.0" text="+" />
         </children>
      </HBox>
   </graphic>
</fx:root>
