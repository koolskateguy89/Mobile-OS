<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.web.WebView?>
<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>

<fx:root prefHeight="475.0" prefWidth="450.0" stylesheets="@WebBrowser.css" type="AnchorPane" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<WebView fx:id="webView" prefHeight="437.0" prefWidth="450.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />
		<HBox alignment="CENTER_LEFT" layoutX="-45.0" layoutY="-24.0" maxHeight="-Infinity" prefHeight="42.0" prefWidth="450.0" spacing="5.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="0.0">
			<children>
				<JFXButton fx:id="back" contentDisplay="GRAPHIC_ONLY" onAction="#back">
               <graphic>
                  <ImageView fitHeight="18.0" fitWidth="86.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@back.png" />
                     </image>
                  </ImageView>
               </graphic></JFXButton>
				<JFXButton fx:id="forward" contentDisplay="GRAPHIC_ONLY" onAction="#forward">
               <graphic>
                  <ImageView fitHeight="18.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@forward.png" />
                     </image>
                  </ImageView>
               </graphic></JFXButton>
				<JFXButton fx:id="reloadBtn" contentDisplay="GRAPHIC_ONLY" onAction="#reload">
               <graphic>
                  <ImageView fitHeight="18.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@reload.png" />
                     </image>
                  </ImageView>
               </graphic></JFXButton>
				<JFXTextField fx:id="addressBar" onAction="#go" prefHeight="26.0" prefWidth="284.0" />
				<Button mnemonicParsing="false" onAction="#go" text="Go" />
			</children>
		</HBox>
	</children>
</fx:root>
